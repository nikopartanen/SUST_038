                <TextEquiv>
                    <Unicode>Einmal gingen sechs männer zum see, um das schleppnetz</Unicode>
                </TextEquiv>
                <TextStyle fontFamily="Times New Roman" fontSize="9.5"/>
            </TextLine>
            <TextLine id="tl_13" primaryLanguage="German" custom="readingOrder {index:1;}">
