                <TextEquiv>
                    <Unicode>die priestersfrau. Die brüder schimpften ihn abermals: »Warum</Unicode>
                </TextEquiv>
                <TextStyle fontFamily="Times New Roman" fontSize="9.0"/>
            </TextLine>
            <TextLine id="tl_34" primaryLanguage="German" custom="readingOrder {index:20;}">
